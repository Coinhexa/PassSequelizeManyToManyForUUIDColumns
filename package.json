{
  "name": "test-rest",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build-ts": "tsc && tsc-alias",
    "build": "npm run build-ts",
    "debug": "npm run build && npm run serve-debug",
    "serve-debug": "nodemon --inspect dist/server/index.js",
    "serve": "node dist/server/index.js",
    "docker:build": "docker-compose up --build",
    "docker:up": "docker-compose up",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "test": "NODE_ENV=test jest --detectOpenHandles --runInBand --verbose --passWithNoTests",
    "coverage": "NODE_ENV=test jest --detectOpenHandles --runInBand --verbose --collect-coverage --passWithNoTests",
    "create-db": "sequelize-cli db:create",
    "drop-db": "sequelize-cli db:drop",
    "apply-migrations": "sequelize-cli db:migrate",
    "revert-migrations": "sequelize-cli db:migrate:undo:all",
    "start": "npm run build && npm run serve"
  },
  "dependencies": {
    "@admin-bro/express": "^3.1.0",
    "@admin-bro/sequelize": "^1.2.1",
    "admin-bro": "^4.0.1",
    "cookie-parser": "~1.4.6",
    "dayjs": "^1.11.3",
    "express": "~4.18.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "express-validation": "^4.1.0",
    "http-errors": "~2.0.0",
    "http-status": "^1.5.2",
    "morgan": "~1.10.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.20.1",
    "sequelize-typescript": "^2.1.3",
    "sqlite3": "^5.0.8",
    "swagger-ui-express": "^4.4.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.2.0",
    "@types/async": "3.2.13",
    "@types/body-parser": "1.19.2",
    "@types/concurrently": "6.4.0",
    "@types/errorhandler": "1.5.0",
    "@types/eslint": "8.4.3",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.1",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "@types/request": "2.48.8",
    "@types/request-promise": "4.1.48",
    "@types/supertest": "2.0.12",
    "@types/validator": "^13.7.3",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "concurrently": "7.2.1",
    "debug": "^4.3.4",
    "eslint": "8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "jest": "^28.1.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "reflect-metadata": "^0.1.13",
    "sass": "1.52.3",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.2.3",
    "ts-jest": "28.0.6",
    "ts-node": "10.8.1",
    "tsc-alias": "^1.6.9",
    "typescript": "4.7.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "collectCoverageFrom": [
      "src/**",
      "!**/tests/**",
      "!**/utils/**",
      "!**/**test**",
      "!**/middlewares/**"
    ],
    "testPathIgnorePatterns": [
      "/.imagine/",
      "<rootDir>/dist/"
    ]
  }
}
